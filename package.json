{
  "name": "drywall",
  "version": "0.7.0-constellation",
  "description": "drywall component library",
  "main": "lib/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "files": [
    "es",
    "lib",
    "src",
    "docs",
    "styleguide.config.js"
  ],
  "scripts": {
